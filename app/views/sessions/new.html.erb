<% provide(:title, "Log in") %>

<%= form_for(:session, url: login_path) do |f| %>
    <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.submit "Log in" %>
    </p>
<% end %>